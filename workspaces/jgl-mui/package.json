{
  "name": "jgl-mui",
  "version": "0.0.1",
  "type": "module",
  "description": "React MUI component library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && npm run build:cjs",
    "build:cjs": "esbuild src/index.ts --bundle --outfile=dist/index.cjs --format=cjs --external:react --external:@mui/material --external:@emotion/react --external:@emotion/styled",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["react", "mui", "components", "table"],
  "author": "Jagel",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "your-repo-url-here"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "@mui/material": "^7.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "esbuild": "^0.19.0",
    "@types/react": "^18.0.0"
  }
}