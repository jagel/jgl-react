    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "rxjs": "^7.8.2",
    "axios": "^1.13.2"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^5.1.1",
    "typescript": "5.9.3",
